{
  "name": "my-qwik-basic-starter",
  "description": "Demo app with sample routes",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build:cf": "npm run build.client && npm run build.server && node scripts/fix-worker-path.js && npm run build.types",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "npm run build:cf && npx wrangler pages publish dist --project-name=qwik-todos --branch=main",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fix:worker": "node scripts/fix-worker-path.js",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev dist --compatibility-flags=nodejs_als",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.16.0",
    "@builder.io/qwik-city": "^1.16.0",
    "@eslint/js": "latest",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "20.19.0",
    "dotenv": "^17.2.2",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.16.0",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "7.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^4.37.1"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250921.0",
    "@libsql/client": "^0.15.15",
    "valibot": "^1.1.0"
  }
}
